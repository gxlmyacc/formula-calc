{
  "name": "formula-calc",
  "version": "0.0.4",
  "description": "formula-calc is a library for formula calculation through strings for javascript.",
  "keywords": [
    "formula",
    "formula-calc",
    "formula-calculator",
    "formula-calculation",
    "formula-calculate",
    "calc",
    "calculator",
    "calculation",
    "calculate",
    "string-calc",
    "string-calculator",
    "string-calculation",
    "string-calculate",
    "string-formula",
    "string-formula-calc",
    "string-formula-calculator",
    "string-formula-calculation",
    "string-formula-calculate",
    "string-formula-calc"
  ],
  "sideEffects": false,
  "main": "esm/index.js",
  "module": "esm/index.js",
  "typings": "types/index.d.ts",
  "engines": {
    "node": "> 6.9.1"
  },
  "files": [
    "esm",
    "es",
    "src",
    "types"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "test-coveralls": "yarn jest --coverage && coveralls < coverage/lcov.info -r hijUIuPWtZpqtkNtwbRTfnqfYNdaRpORz",
    "build": "npm run build:types && npm run build:js && npm run build:es",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "cross-env BUILD_ENV=js babel src -d esm --extensions \".ts,.tsx\" --source-maps",
    "build:es": "cross-env BUILD_ENV=es babel src -d es --extensions \".ts,.tsx\"",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gxlmyacc/formula-calc"
  },
  "author": "gxlmyacc",
  "dependencies": {
    "core-js": "^2",
    "decimal.js": "^10.4.3",
    "tslib": "^2.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
